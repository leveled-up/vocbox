{
  "database": {
    "rules": "database/database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/a/**",
      "function": "app"
    }, {
      "source": "/",
      "function": "app"
    }]
  }
}
